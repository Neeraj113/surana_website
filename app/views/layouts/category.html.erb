        <script>
            function initialize() {
                var f = document.getElementById("map-canvas");
                if(f){
                    var b = {
                        center: new google.maps.LatLng(17.712446, 83.29975),
                        zoom: 16,
                        scrollwheel: false,
                        mapTypeId: google.maps.MapTypeId.ROADMAP
                    };
                    var e = new google.maps.Map(f, b);
                    var d = new google.maps.InfoWindow();
                    var a, c;
                    a = new google.maps.Marker({
                        position: new google.maps.LatLng(17.712446, 83.29975),
                        map: e
                    });
                    d.setContent('<div style="overflow: auto;"><strong>Surana Sanitations</strong><br>28-9-32, Suryabagh, Vizag<br><a style="color:#427fed;" href="https://www.google.com/maps/place/Surana+Sanitations/@17.712494,83.2976383,17z/data=!3m1!4b1!4m5!3m4!1s0x3a3969b1dfffbd55:0xefa68cd712aedf8c!8m2!3d17.712494!4d83.299827" target="_blank">View on Google Maps</a></div>');
                    d.open(e, a);
                    google.maps.event.addListenerOnce(a, "click", (function(g, h) {
                        return function() {
                            d.open(e, g)
                        }
                    })(a, c))
                }
            }

            if(window.google){
              initialize();
            } else{
              $.ajax('https://maps.googleapis.com/maps/api/js?callback=initialize&hl=en&render=explicit', {
                crossDomain: true,
                'data-turbolinks-eval' : false,
                dataType: 'script'
              });
            }

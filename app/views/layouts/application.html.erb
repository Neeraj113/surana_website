<!DOCTYPE html>
<html>
  <head>
    <title>SuranaWebsite</title>
    <%= csrf_meta_tags %>
    <% if @current_user %>
      <%= link_to "Log out", logout_path, method: :delete %>
    <% end %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
